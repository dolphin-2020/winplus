@model WebApplication5.Models.Client
@{
    ViewBag.Title = "ChangdeClientInfo";
}

<h2>ChangdeClientInfo</h2>

<div>
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary();
        @Html.LabelFor(c => c.Name)
        <br />
        @Html.TextBoxFor(c => c.Name)
        <br />
        @Html.LabelFor(c => c.PhoneNum)
        <br />
        @Html.TextBoxFor(c => c.PhoneNum)
        <br />
        @Html.LabelFor(c => c.Email)
        <br />
        @Html.TextBoxFor(c => c.Email)
        <br />
        @Html.LabelFor(c => c.Address)
        <br />
        @Html.TextBoxFor(c => c.Address)
        <br />
        @Html.LabelFor(c=>c.Password)
        <br />
        @Html.TextBoxFor(c=>c.Password)
        <input type="submit"value="Change" />
    }

</div>